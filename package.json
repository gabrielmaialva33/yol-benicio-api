    "ts-node-maintained": "^10.9.6",
    "typescript": "~5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.1.1",
    "@adonisjs/auth": "^9.4.2",
