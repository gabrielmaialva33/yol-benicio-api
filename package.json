    "zod": "^4.1.8"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "@tailwindcss/oxide",
      "argon2",
      "bcrypt",
      "esbuild",
      "msgpackr-extract",
