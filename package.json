    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@google-cloud/storage": "^7.17.1",
    "@heroicons/react": "^2.2.0",
    "@inertiajs/react": "^2.1.6",
    "@rlanz/bull-queue": "^3.1.0",
    "@tanstack/react-query": "^5.87.4",
